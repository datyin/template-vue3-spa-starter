{
  "name": "template-vue3-ts",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "description": "Starter template for Vue 3 with TypeScript",
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "contributors": [
    {
      "name": "Jove Jonovski",
      "email": "jovejonovski@gmail.com",
      "url": "https://github.com/datyin"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "vue-tsc --noEmit",
    "dev": "vite",
    "build": "pnpm run lint && pnpm run ts:check && vite build"
  },
  "dependencies": {
    "@vueuse/core": "13.1.0",
    "naive-ui": "2.41.0",
    "pinia": "3.0.2",
    "vue": "3.5.13",
    "vue-router": "4.5.1",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.12.0",
    "@iconify/json": "2.2.331",
    "@types/node": "22.15.2",
    "@unocss/eslint-plugin": "66.1.0-beta.12",
    "@unocss/preset-web-fonts": "66.1.0-beta.12",
    "@unocss/reset": "66.1.0-beta.12",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.25.1",
    "eslint-plugin-format": "1.0.1",
    "typescript": "5.8.3",
    "unocss": "66.1.0-beta.12",
    "unplugin-icons": "22.1.0",
    "unplugin-vue-markdown": "28.3.1",
    "vite": "6.3.3",
    "vite-plugin-vue-devtools": "7.7.5",
    "vue-i18n": "11.1.3",
    "vue-tsc": "2.2.10"
  }
}
